event = {
    eventId: tstr
    source: tstr
    eventName: tstr
    eventTime: timestamp
    paramSet: [* parameter]
}

parameter = {
    keyName: tstr
    keyType: keytype-choices
    items: [* radec ] /
           [* tstr ] /
           [* struct ] /
           [* timestamp ] /
           [* bool ] /
           bstr /
           [* int ] /
           [* float32 ] /
           [* float64 ] /

           [* bstr ] /
           [* [* int ] ] /
           [* [* float32 ] ] /
           [* [* float64 ] ] /

           [* [* bstr ] ] /
           [* [* [* int ] ] ] /
           [* [* [* float32 ] ] ] /
           [* [* [* float64 ] ] ]
    units: unit-choices
}

struct = {
    paramSet: [* parameter]
}

timestamp = {
    seconds: int
    nanos: int
}

radec = {
    ra: float64
    dec: float64
}

