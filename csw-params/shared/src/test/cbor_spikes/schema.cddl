event = {
    eventId: tstr
    source: tstr
    eventName: tstr
    eventTime: timestamp
    paramSet: [* parameter]
}

timestamp = {
    seconds: int
    nanos: int
}

radec = {
    ra: float64
    dec: float64
}

parameter = {
    keyName: tstr
    keyType: tstr
    items: [* radec ] /
           [* tstr ] /
           [* parameter ] /
           [* timestamp ] /
           [* bool ] /
           bstr /
           [* int ] /
           [* float32 ] /
           [* float64 ] /

           [* bstr ] /
           [* [* int ] ] /
           [* [* float32 ] ] /
           [* [* float64 ] ] /

           [* [* bstr ] ] /
           [* [* [* int ] ] ] /
           [* [* [* float32 ] ] ] /
           [* [* [* float64 ] ] ] /
    units: tstr
}
