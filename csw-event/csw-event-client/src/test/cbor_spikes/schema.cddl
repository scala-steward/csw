event = {
    eventId: tstr
    source: tstr
    eventName: tstr
    eventTime: timestamp
    paramSet: [* parameter]
}

timestamp = {
    seconds: int
    nanos: int
}

radec = {
    ra: float64
    dec: float64
}

parameter = {
    keyName: tstr
    keyType: tstr
    items: [* int ] /
           [* float32 ] /
           [* float64 ] /
           [* bool ] /
           [* tstr ] /
           [* bstr ] /
           [* timestamp ] /
           [* radec ] /
           [* parameter ] /

           [* [* int ] ] /
           [* [* float32 ] ] /
           [* [* float64 ] ] /
           [* [* bool ] ] /
           [* [* tstr ] ] /
           [* [* bstr ] ] /
           [* [* timestamp ] ] /
           [* [* radec ] ] /
           [* [* parameter ] ] /

           [* [* [* int ] ] ] /
           [* [* [* float32 ] ] ] /
           [* [* [* float64 ] ] ] /
           [* [* [* bool ] ] ] /
           [* [* [* tstr ] ] ] /
           [* [* [* bstr ] ] ] /
           [* [* [* timestamp ] ] ] /
           [* [* [* radec ] ] ] /
           [* [* [* parameter ] ] ]

    units: tstr
}
